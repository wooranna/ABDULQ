          "_name": "slider5",
          "_customHTML": "<section class=\"slider5 slider\" group=\"Sliders\" plugins=\"amp-main,amp-carousel\">\n\t<mbr-parameters>\n\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t<header>Size</header>\n\t<input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n\t<input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n\t<header>Slider</header>\n\t<input type=\"checkbox\" title=\"Show Slider\" name=\"showSlider\" checked=\"\">\n\t<select title=\"Images\" name=\"imagesAmount\" condition=\"showSlider\">\n\t\t<option value=\"2\">2</option>\n\t\t<option value=\"3\" selected=\"\">3</option>\n\t</select>\n\t<input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\" condition=\"imagesAmount == 3 &amp;&amp; showSlider\" checked=\"\">\n\t<input type=\"range\" inline=\"\" title=\"Delay\" name=\"delay\" min=\"1000\" max=\"10000\" step=\"1000\" value=\"3000\" condition=\"autoplay &amp;&amp; showSlider\">\n\t<input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type!='color'\">\n\t<input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#141229\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n\t<input type=\"range\" inline=\"\" title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n\t<header>Content</header>\n\t<input type=\"checkbox\" title=\"Content Block\" name=\"showCont\" checked=\"\">\n\t<input type=\"checkbox\" title=\"Tag\" name=\"showTag\" checked=\"\" condition=\"showCont\">\n    <input type=\"checkbox\" title=\"Date\" name=\"showDate\" checked=\"\" condition=\"showCont\">\n    <input type=\"checkbox\" title=\"Title\" name=\"showArtTitle\" checked=\"\" condition=\"showCont\">\n    <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked=\"\" condition=\"showCont\">\n\t<header>Icons</header>\n    <input type=\"checkbox\" title=\"Show Icons\" name=\"showIcons\" checked=\"\">\n    <input type=\"range\" title=\"Icons Size\" name=\"iconsSize\" min=\"1\" max=\"3\" step=\"0.25\" value=\"2.5\" condition=\"showIcons\">\n    <input type=\"range\" title=\"Icons Amount\" name=\"iconsAmount\" min=\"1\" max=\"5\" step=\"1\" value=\"5\" condition=\"showIcons\">\n    <header>Progress Bars</header>\n\t<!--  -->\n\t<input type=\"checkbox\" title=\"Progress Block\" name=\"showBars\" checked=\"\">\n\t<input type=\"range\" title=\"Amount of <br> Progress Bars\" name=\"progressAmount\" min=\"1\" max=\"5\" step=\"1\" value=\"5\" condition=\"showBars\">\n\t<input type=\"color\" title=\"Progress Counter Color\" name=\"countColor\" value=\"#1bc2ca\" condition=\"showBars\">\n\t<input type=\"range\" inline=\"\" title=\"Progress 1\" name=\"progress1\" min=\"0\" max=\"100\" step=\"1\" value=\"35\" condition=\"showBars\">\n\t<input type=\"color\" title=\"Progress 1 Value Color\" name=\"progress1Color\" value=\"#5e49d5\" condition=\"showBars\">\n\t<!--  -->\n\t<input type=\"range\" inline=\"\" title=\"Progress 2\" name=\"progress2\" min=\"0\" max=\"100\" step=\"1\" value=\"56\" condition=\"showBars\">\n\t<input type=\"color\" title=\"Progress 2 Value Color\" name=\"progress2Color\" value=\"#5e49d5\" condition=\"showBars\">\n\t<!--  -->\n\t<input type=\"range\" inline=\"\" title=\"Progress 3\" name=\"progress3\" min=\"0\" max=\"100\" step=\"1\" value=\"65\" condition=\"showBars\">\n\t<input type=\"color\" title=\"Progress 3 Value Color\" name=\"progress3Color\" value=\"#5e49d5\" condition=\"showBars\">\n\t<!--  -->\n\t<input type=\"range\" inline=\"\" title=\"Progress 4\" name=\"progress4\" min=\"0\" max=\"100\" step=\"1\" value=\"100\" condition=\"showBars\">\n\t<input type=\"color\" title=\"Progress 4 Value Color\" name=\"progress4Color\" value=\"#1bc2ca\" condition=\"showBars\">\n\t<!--  -->\n\t<input type=\"range\" inline=\"\" title=\"Progress 5\" name=\"progress5\" min=\"0\" 